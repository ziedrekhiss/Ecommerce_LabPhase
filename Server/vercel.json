{
  "version": 2,
  "builds": [{ "src": "./server.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/user/(.*)", "dest": "/api/user/$1" },
    { "src": "/api/product/(.*)", "dest": "/api/product/$1" },
    { "src": "/api/cart/(.*)", "dest": "/api/cart/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
