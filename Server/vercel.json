{
  "version": 2,
  "builds": [{ "src": "./server.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/cart", "dest": "/cart" },
    { "src": "/order", "dest": "/order" },
    { "src": "/product", "dest": "/product" },
    { "src": "/user", "dest": "/user" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
